{
  "name": "@lit-protocol/law-cli",
  "version": "0.1.0-19",
  "publishConfig": {
    "access": "public"
  },
  "bin": {
    "law": "./dist/src/index.js"
  },
  "type": "commonjs",
  "main": "./dist/src/index.js",
  "types": "./dist/src/index.d.ts",
  "typings": "./dist/src/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/src/index.d.ts",
      "require": "./dist/src/index.js",
      "default": "./dist/src/index.js"
    }
  },
  "files": [
    "dist",
    "!**/*.tsbuildinfo"
  ],
  "dependencies": {
    "@lit-protocol/constants": "7.0.2",
    "ethers": "v5",
    "node-localstorage": "^3.0.5",
    "prompts": "^2.4.2",
    "tslib": "^2.3.0",
    "zod": "^3.24.1",
    "@lit-protocol/agent-wallet": "0.1.0-19"
  },
  "devDependencies": {
    "@lit-protocol/types": "7.0.2",
    "@types/node-localstorage": "^1.3.3",
    "@types/prompts": "^2.4.9"
  },
  "nx": {
    "sourceRoot": "packages/law-cli/src",
    "projectType": "library",
    "targets": {
      "build": {
        "executor": "@nx/js:tsc",
        "outputs": [
          "{options.outputPath}"
        ],
        "options": {
          "outputPath": "packages/law-cli/dist",
          "main": "packages/law-cli/src/index.ts",
          "tsConfig": "packages/law-cli/tsconfig.lib.json",
          "assets": [
            "packages/law-cli/*.md"
          ]
        }
      }
    },
    "name": "law-cli"
  }
}