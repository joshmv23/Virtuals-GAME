#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/joshmorfin-vazquez/Virtuals-GAME/node_modules/.pnpm/hardhat@2.22.18_ts-node@10.9.2_@types+node@22.13.4_typescript@5.7.3__typescript@5.7.3/node_modules/hardhat/internal/cli/node_modules:/Users/joshmorfin-vazquez/Virtuals-GAME/node_modules/.pnpm/hardhat@2.22.18_ts-node@10.9.2_@types+node@22.13.4_typescript@5.7.3__typescript@5.7.3/node_modules/hardhat/internal/node_modules:/Users/joshmorfin-vazquez/Virtuals-GAME/node_modules/.pnpm/hardhat@2.22.18_ts-node@10.9.2_@types+node@22.13.4_typescript@5.7.3__typescript@5.7.3/node_modules/hardhat/node_modules:/Users/joshmorfin-vazquez/Virtuals-GAME/node_modules/.pnpm/hardhat@2.22.18_ts-node@10.9.2_@types+node@22.13.4_typescript@5.7.3__typescript@5.7.3/node_modules:/Users/joshmorfin-vazquez/Virtuals-GAME/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/joshmorfin-vazquez/Virtuals-GAME/node_modules/.pnpm/hardhat@2.22.18_ts-node@10.9.2_@types+node@22.13.4_typescript@5.7.3__typescript@5.7.3/node_modules/hardhat/internal/cli/node_modules:/Users/joshmorfin-vazquez/Virtuals-GAME/node_modules/.pnpm/hardhat@2.22.18_ts-node@10.9.2_@types+node@22.13.4_typescript@5.7.3__typescript@5.7.3/node_modules/hardhat/internal/node_modules:/Users/joshmorfin-vazquez/Virtuals-GAME/node_modules/.pnpm/hardhat@2.22.18_ts-node@10.9.2_@types+node@22.13.4_typescript@5.7.3__typescript@5.7.3/node_modules/hardhat/node_modules:/Users/joshmorfin-vazquez/Virtuals-GAME/node_modules/.pnpm/hardhat@2.22.18_ts-node@10.9.2_@types+node@22.13.4_typescript@5.7.3__typescript@5.7.3/node_modules:/Users/joshmorfin-vazquez/Virtuals-GAME/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../internal/cli/bootstrap.js" "$@"
else
  exec node  "$basedir/../../internal/cli/bootstrap.js" "$@"
fi
